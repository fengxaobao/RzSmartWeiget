<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/common_cl_white" />
            <stroke android:width="@dimen/dp_1" android:color="@color/color_f4f3" />
            <corners android:radius="@dimen/dp_4" />
            <size android:width="@dimen/dp_78" android:height="@dimen/dp_78" />
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/blur_primary" />
            <stroke android:width="@dimen/dp_1" android:color="@color/color_f4f3" />
            <corners android:radius="@dimen/dp_4" />
            <size android:width="@dimen/dp_78" android:height="@dimen/dp_78" />
        </shape>
    </item>

</selector>